

#finding length of dictionary in python


month=dict()
print "\n An empty dictionary:",month

#assigining value at a string index of dictionary
month["one"]="January"
month["two"]="Feb"
month["three"]="March"
month["Four"]="Apirl"
month["Five"]="May"
month["Six"]="June"
month["Seven"]="July"

print "\n Dictionary with string index:",month
print"\n Length of Dictionary-1:",len(month)                             #determining length of dictionary month

#assigining value at a integer index of dictionary
t=dict()
t[0]="January"
t[1]="Feb"
t[2]="March"
t[3]="Apirl"

print "\n Dictionary with integer index:",t
print"\n Length of Dictionary-2:",len(t)                             #determining length of dictionary t

#assigining value at a alphanumeric index of dictionary
x=dict()
x["one"]="January"
x["2"]="Feb"

print "\n Dictionary with alphanumeric index:",x
print"\n Length of Dictionary-3:",len(x)                             #determining length of dictionary x
